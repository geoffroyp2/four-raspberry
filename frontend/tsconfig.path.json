{
  "compilerOptions": {
    "baseUrl": "./src/",
    "paths": {
      "@store": ["app/store.ts"],
      "@apiTypes/*": ["app/types/*"],

      "@assets/*": ["assets/*"],
      "@canvasjs": ["assets/canvasjs-3.2.17/canvasjs.react.js"],

      "@svg/*": ["components/svg/*"],
      "@tables/*": ["components/tables/*"],
      "@cards/*": ["components/cards/*"],
      "@buttons/*": ["components/buttons/*"],
      "@inputs/*": ["components/inputs/*"],

      "@editor/*": ["app/editor/*"],
      "@header/*": ["app/header/*"],
      "@formulas/*": ["app/editor/formulas/*"],
      "@graphs/*": ["app/editor/graphs/*"],
      "@pieces/*": ["app/editor/pieces/*"],
      "@app/*": ["app/*"],
      "@src/*": ["*"]
    }
  }
}
